{
    "title": "极客时间付费专栏备份",
    "description": "极客时间付费专栏备份",
    "author": "zerolee",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-lunr", "-search", "search-plus", "search-pro",
        "-highlight","prism","prism-themes",
        "github",
        "copy-code-button",
        "splitter",
        "-sharing",
        "tbfed-pagefooter",
        "anchors",
        "simple-page-toc",
        "hide-element",
        "sectionx",
        "back-to-top-button",
        "page-toc-button",
        "cuav-chapters",
        "heading-anchors",
        "fancybox",
        "popup",
        "multipart",
        "book-summary-scroll-position-saver",
        "todo",
        "url-embed"
    ],

    "pluginsConfig": {
        "sytles": {
            "theme": "night"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "fontsettings": {
            "theme": "night",
            "family": "sans",
            "size": 1
        },
        "github": {
            "url": "https://github.com/zerolee1993"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ],
            "lang": {
                "shell": "bash"
            }
        },
        "theme-default": {
            "showLevel": false
        },
        "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "sectionx": {
            "tag": "b"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        }
    }
}
